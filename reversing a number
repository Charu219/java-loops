import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in=new Scanner(System.in);
        int num=in.nextInt();
        int rev;
        int  sum=0;
        while(num!=0){
            rev=num%10;
        sum=sum*10+rev;
        num=num/10;
        }
        System.out.println(sum);
    }
}
