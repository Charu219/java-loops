import java.util.Scanner;
public class Main{
public static void main(String[] args){
    Scanner in=new Scanner(System.in);
    int num1=in.nextInt();
    int num2=in.nextInt();
    int count=0;
int small=Math.min(num1,num2);
for(int i=1;i<=small;i++)
{

    if(num1%i==0 && num2%i==0){
        count++;
    }
}
System.out.println(count);
    }
}
