
public class Main{
    public static void main(String[] args){
       
        try{
           String text=null;
          System.out.println(text.length());
        }
        catch(NullPointerException e){
            System.out.println("error is " +e.getMessage());
        }
        
        finally{
            System.out.println("always runs");
        }
    }
}

