import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in=new Scanner(System.in);
        String word=in.nextLine();
        String[] parts=word.split(" ");
        String rev="";
        
        
        for(int i=parts.length-1;i>=0;i--){
            
            rev=rev+parts[i];

           if(i!=0){
               rev=rev+" ";
           }
            
          }
        System.out.println(rev);
    }
}
