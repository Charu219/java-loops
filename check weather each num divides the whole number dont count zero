import java.util.Scanner;
public class Main{
public static void main(String[] args){
    Scanner in=new Scanner(System.in);
    int num=in.nextInt();
    int count=0;
int num1=num;
while(num1!=0){
   int div= num1%10;
   if( div!=0 && num%div==0 ){
       count++;
   }
    
  num1=  num1/10;
}
System.out.println(count);
    }
}
