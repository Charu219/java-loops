import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in=new Scanner(System.in);
        String word=in.nextLine();
      
        String rev="";
        
        
        for(int i=word.length()-1;i>=0;i--){
            
            rev=rev+word.charAt(i);

           
          }
        System.out.println(rev);
    }
}

input--> cute doll
output-->llod etuc
